package poldercast.subscriptions;

/**
 * 
 * @author anca
 * 
 */

public interface Topics
{
  /*
   * Method to get topics to be assigned to a node with id nodeIndex
   */
  Object getTopics(int nodeIndex);
  
  /*
   * Method to get nodes to be subscribed to a topic with topicIndex
   */
  Object getNodes(int topicIndex);
}
